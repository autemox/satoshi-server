{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch TS Project",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
            "args": ["${workspaceFolder}/3016-satoshi-server.ts"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
              "${workspaceFolder}/**/*.js",
              "!${workspaceFolder}/node_modules/typescript/**/*.js"
            ],
            "protocol": "inspector"
        }
    ]
}
